
   <view>
      
    <swiper class="swiper" indicator-dots="true" autoplay="true" interval="5000" duration="1000">
      <view class="show">
        <block  wx:for="{{$imageshow$movies}}" wx:key="unique">
          <swiper-item>
            <image src="{{item}}" mode="aspectFill"/>
          </swiper-item>
        </block>
      </view>
    </swiper>

    </view>
    <view>
      
  <view class="panel {{$Panel$num > 55 ? 'grey' : ''}} {{$Panel$num < 45 ? 'white' : ''}}" >
    <text class="title">华邦动态</text>
    
        
        <text class="subtitle">持之以恒，不为私心所困，为实现互联网事业的梦想而奋斗</text>
      
  </view>

      
  <view class="articlelist">
    <view class=""  wx:for="{{$Articlelist$moves}}" wx:key="unique">
      <view class="articledata">
        <view class="day"><text>{{item.day}}</text></view>
        <view class="otherdata"><text>{{item.otherdata}}</text></view>
      </view>
      <view class="article">
          <view class="title"><text>{{item.title}}</text></view>
          <view class="profile"><text>{{item.profile}}</text></view>
      </view>
    </view>
    <view class="morearticle">
      <button type="default">查看更多</button>
    </view>
  </view>

    </view>